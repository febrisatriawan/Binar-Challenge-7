<!DOCTYPE html>
<html lang="en">
  <%- include ("partials/head", { title: "Games", href: "/assets/game.css"}); %>

<body>
  <!-- Navigation -->
  <%- include ("partials/navbar"); %>
 
  <!-- Game Area -->
  <main>
    <div class="container">
      <div class="game">
        <div id="player">
          <h2> <%= name %></h2>
          <img class="rock" src="//batu.png" alt="rock">
          <img class="paper" src="/img/kertas.png" alt="paper">
          <img class="scissors" src="/img/gunting.png" alt="scissors">
        </div>
        <div class="info-board">
          <span class="vs">VS</span>
        </div>
        <div id="computer">
          <h2>Computer</h2>
          <img class="rock" src="/img/batu.png" alt="rock">
          <img class="paper" src="/img/kertas.png" alt="paper">
          <img class="scissors" src="/img/gunting.png" alt="scissors">
        </div>
      </div>
      <div class="restart">
        <img class="reset" src="/img/refresh.png" alt="restart game">
      </div>
    </div>

  </main>
 
  <!-- Link Scripts Js  -->
  <script src="/asset/game.js"></script>
</body>

</html>