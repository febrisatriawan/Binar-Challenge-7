<!-- Section Games -->
    <section id="games" class="games">
      <div class="container">
        <div class="row">
          <div class="col-md-12 col-xl-4 px-5 px-md-2">
            <div class="games__title text-center text-xl-start">
              <h5 class="title-small">What's so special?</h5>
                <h1 class="title-big">THE GAMES</h1>
            </div>
          </div>
            <div class="col-md-12 col-xl-8">
              <div class="games__slider">
                <div id="Indicatordot" class="carousel slide" data-bs-ride="carousel">
                  <div class="carousel-indicators">
                    <button type="button" data-bs-target="#Indicatordot" data-bs-slide-to="0" class="active rounded-circle"
                      aria-label="Slide 1" aria-current="true"></button>
                    <button type="button" data-bs-target="#Indicatordot" data-bs-slide-to="1" aria-label="Slide 2"
                      class="rounded-circle"></button>
                    <button type="button" data-bs-target="#Indicatordot" data-bs-slide-to="2" aria-label="Slide 3"
                      class="rounded-circle"></button>
                    <button type="button" data-bs-target="#Indicatordot" data-bs-slide-to="3" aria-label="Slide 4"
                      class="rounded-circle"></button>
                  </div>
                    <div class="carousel-inner">
                      <div class="carousel-item active">
                        <img src="/img/gunting.jpg" class="d-block w-100" alt="slide 1">
                      </div>
                      <div class="carousel-item">
                        <img src="/img/gunting2.jpg" class="d-block w-100" alt="slide 2">
                      </div>
                        <img src="/img/gunting.jpg" class="d-block w-100" alt="slide 3">
                      </div>
                      <div class="carousel-item">
                        <img src="/img/gunting2.jpg" class="d-block w-100" alt="slide 4">
                      </div>
                </div>
                      <button class="carousel-control-prev icon-costum-left" type="button" data-bs-target="#Indicatordot"
                        data-bs-slide="prev">                                
                          <span class="visually-hidden">Previous</span>
                            <img src="/img/btnkr.png">
                      </button>
                      <button class="carousel-control-next icon-costum-right" type="button" data-bs-target="#Indicatordot"
                        data-bs-slide="next">                                
                          <span class="visually-hidden">Next</span>
                            <img src="/img/btnkn.png">
                      </button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
<!-- End Section Games -->

<!-- Section Features -->
    <section id="features" class="features">
      <div class="container">
        <div class="row">
          <div class="col-md-6 col-lg-5 offset-md-6 offset-lg-7 px-5 px-md-2">
            <div class="features__title text-center text-md-start mb-5 mb-md-0">
              <h5 class="title-small">What's so special?</h5>
              <h1 class="title-big">features</h1>
        </div>
        <div class="features__list">
          <ul>
            <li>
              <span class="icon-list active"></span>
                <div class="content-list">
                  <p class="title-big-secondary">traditional game</p>
                    <p class="text-list">if you miss your childhood, we provide many traditional games here</p>
                </div>
            </li>
            <li>
              <span class="icon-list"></span>
                <div class="content-list">
                  <p class="title-big-secondary">game suit</p>
                </div>
            </li>
            <li>
              <span class="icon-list"></span>
                <div class="content-list">
                  <p class="title-big-secondary">tbd</p>
                </div>
              </li>
            </ul>
          </div>
        </div>
      </div>
      </div>
    </section>
