<!-- Section System -->
    <section id="system" class="system">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <h2 class="text-white text-center">Can My Computer Run this game ?</h2>
          </div>
        </div>
      <div class="row">
        <div class="col-md-8 col-lg-8 col-xl-6 px-5 px-md-2">
          <div class="system__title text-center text-md-start">
            <h1 class="title-big">SYSTEM REQUIRMENTS</h1>
          </div>
        <div class="system__requirments">
          <table class="table table-bordered">
            <tr>
              <td class="left">
                <p class="title-big-secondary">OS</p>
                <p class="text-list">Windows 7 64-bit only (no OSX support this time</p>
              </td>
              <td class="right">
                <p class="title-big-secondary">PROCESSOR</p>
                <p class="text-list">Intel Core 2 Duo @ 2.4 GHZ or AMD Athlon X2 @ 2.8 GHZ</p>
              </td>
            </tr>
            <tr>
              <td class="left">
                <p class="title-big-secondary">MEMORY</p>
                <p class="text-list">4 GB RAM</p>
              </td>
              <td class="right">
                <p class="title-big-secondary">STORAGE</p>
                <p class="text-list">8 GB available space</p>
              </td>
            </tr>
            <tr>
              <td class="left" colspan="2">
                <p class="title-big-secondary">GRAPHIC</p>
                <p class="text-list">NVIDIA GeForce GTX 660 2GB or AMD Radeon HD 7850 2GB DirectX11 (Shader Model 5)</p>
              </td>
            </tr>
          </table>
        </div>
        </div>
      </div>
      </div>
    </section>

<!-- Section Score -->
    <section id="score" class="score">
