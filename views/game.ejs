<!DOCTYPE html>
<html lang="en">
  <%- include ("partials/head", { title: "Traditional Games", href: "/asset/game.css" }); %>
<body>

  <!-- Navigation -->
  <nav>
    <div class="back">
      <a href="/"> < </a>
    </div>
    <div class="favicon">
      <img src="/img/favicon.png" alt="logo">
    </div>
    <p>Rock Paper Scissors</p>
  </nav>
 
  <!-- Game Area -->
  <main>
    <div class="container">
      <div class="game">
        <div id="player">
          <h2>Player</h2>
          <img class="rock" src="/img/batu.png" alt="rock">
          <img class="paper" src="/img/kertas.png" alt="paper">
          <img class="scissors" src="/img/gunting.png" alt="scissors">
        </div>
        <div class="info-board">
          <span class="vs">VS</span>
        </div>
        <div id="computer">
          <h2>Computer</h2>
          <img class="rock" src="/img/batu.png" alt="rock">
          <img class="paper" src="/img/kertas.png" alt="paper">
          <img class="scissors" src="/img/gunting.png" alt="scissors">
        </div>
      </div>
      <div class="restart">
        <img class="reset" src="/img/refresh.png" alt="restart game">
      </div>
    </div>

  </main>
 
  <!-- Link Scripts Js  -->
  <script src="/asset/game.js"></script>
</body>

</html>